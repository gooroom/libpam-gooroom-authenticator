#: src/pam.c:1433
msgid "Failed to connect to server"
msgstr "서버 연결에 실패하였습니다."

#: src/pam.c:1436
msgid "Operation timeout"
msgstr "서버 연결 시간이 초과하였습니다."

#: src/pam.c:1439
msgid "Connection error"
msgstr "서버 연결 에러가 발생하였습니다."

#: src/pam.c:2053
msgid "Failure of the Two-Factor Authentication"
msgstr ""
"이중 인증에 실패하였습니다.\n"
"인증을 다시 시도 하시기 바랍니다."

#: src/pam.c:2067
msgid "Failed to rewrap passphrase for ecryptfs"
msgstr "암호화 파일시스템에 연결할 수 없습니다."

#: src/pam.c:2265
msgid "Current password: "
msgstr "현재  암호:"

#: src/pam.c:2296
msgid "New password: "
msgstr "새  암호:"

#: src/pam.c:2301
msgid "Retype new password: "
msgstr "새  암호 재입력:"

#: src/pam.c:2308
msgid "Passwords do not match."
msgstr "암호가 일치하지 않습니다."

#: src/nfc-auth.c:223 src/nfc-auth.c:247 src/nfc-auth.c:263
msgid "Not enough memory"
msgstr "메모리 부족"

#: src/nfc-auth.c:240 src/nfc-auth.c:290 src/nfc-auth.c:382
msgid "Reader is not available."
msgstr "카드 리더기를 찾을 수 없습니다."

#: src/nfc-auth.c:318
msgid "Ignore this reader."
msgstr "이 카드 리더기를 무시합니다."

#: src/nfc-auth.c:323
msgid "Unknown Reader."
msgstr "알수없는 카드 리더기 입니다."

#: src/nfc-auth.c:328
msgid "Status unavailable."
msgstr "상태가 유효하지 않습니다."

#: src/nfc-auth.c:334
msgid "Tap your card."
msgstr "추가 인증을 위해 카드를 리더기에 올려 주십시오."

#: src/nfc-auth.c:352
msgid "ATR matches card."
msgstr "ATR이 카드와 일치합니다."

#: src/nfc-auth.c:357
msgid "Exclusive Mode."
msgstr "Exclusive 모드"

#: src/nfc-auth.c:362
msgid "Shared Mode."
msgstr "Shared 모드"

#: src/nfc-auth.c:367
msgid "Unresponsive card."
msgstr "카드가 응답하지 않습니다."

#: src/nfc-auth.c:376
msgid "Input Timeout."
msgstr ""
"입력 시간이 초과 되었습니다.\n"
"인증을 다시 시도 하시기 바랍니다."
